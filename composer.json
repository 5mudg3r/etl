{
  "name": "5mudg3r/etl",
  "description": "An ETL library for PHP.",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "5mudg3r",
      "email": "smudja06@gmail.com"
    }
  ],
  "require": {
    "php": "^7.4"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^2.16",
    "phpunit/phpunit": "^9"
  },
  "autoload": {
    "psr-4": {
      "Smudja\\ETL\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Smudja\\ETL\\Tests\\": "tests/"
    }
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  },
  "prefer-stable": true
}
